{
  "name": "cloud-video-streaming-frontend",
  "version": "1.0.0",
  "description": "Frontend for Cloud Video Streaming Platform with Real-time Monitoring",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --entry-file=index.html",
    "build": "npm run clean && npm run copy-assets",
    "clean": "rimraf dist",
    "copy-assets": "mkdir -p dist && cp -r src/* dist/ && cp index.html dist/",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "serve": "http-server -p 8080 -c-1"
  },
  "keywords": [
    "video-streaming",
    "monitoring",
    "dashboard",
    "real-time",
    "cloud"
  ],
  "author": "Cloud Video Network Monitoring Team",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "http-server": "^14.1.1",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.0",
    "socket.io-client": "^4.7.4",
    "axios": "^1.6.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blackkolly/cloud-video-monitoring"
  },
  "bugs": {
    "url": "https://github.com/blackkolly/cloud-video-monitoring/issues"
  },
  "homepage": "https://github.com/blackkolly/cloud-video-monitoring#readme"
}
